    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.11.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.4",
    "ts-loader": "^6.0.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^2.8.0",
    "@types/googlemaps": "^3.36.5",
    "@types/jest": "^18.1.1",
